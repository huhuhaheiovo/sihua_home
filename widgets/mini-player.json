{
  "type": "AdaptiveCard",
  "body": [
    {
      "type": "TextBlock",
      "size": "Medium",
      "text": "No music playing",
      "horizontalAlignment": "Center",
      "$when": "${!playing}"
    },
    {
      "type": "TextBlock",
      "size": "Medium",
      "text": "Currently playing...",
      "horizontalAlignment": "Center",
      "$when": "${playing}"
    },
    {
      "type": "Image",
      "spacing": "Medium",
      "horizontalAlignment": "Center",
      "size": "Large",
      "url": "${artworkUrl}",
      "$when": "${playing}"
    },
    {
      "type": "TextBlock",
      "spacing": "Large",
      "weight": "Bolder",
      "horizontalAlignment": "Center",
      "text": "${song}, by ${artist}",
      "$when": "${playing}"
    }
  ],
  "actions": [
    {
      "type": "Action.Execute",
      "title": "Previous",
      "verb": "previous",
      "style": "positive",
      "iconUrl": "data:image/png;base64,"
    }
  ],
  "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "version": "1.5"
}